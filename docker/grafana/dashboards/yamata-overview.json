                "x": 12,
                "y": 8
            },
            "options": {
                "reduceOptions": {
                    "calcs": [
                        "lastNotNull"
                    ],
                    "fields": ""
                },
                "orientation": "auto"
            },
            "targets": [
                {
                    "expr": "http_inflight_requests",
                    "refId": "A",
                    "datasource": {
                        "type": "prometheus",
                        "uid": "Prometheus"
                    }
